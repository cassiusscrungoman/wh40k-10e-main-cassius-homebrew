        <profile name="IMC Pilot" typeId="c547-1836-d8a-ff4f" typeName="Unit" hidden="false" id="acc2-294c-4093-d91e">
          <characteristics>
            <characteristic name="M" typeId="e703-ecb6-5ce7-aec1">10&quot;</characteristic>
